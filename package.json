{
  "name": "my-dish",
  "version": "1.0.0-beta.12",
  "description": "Generates various templates. Includes `web application with React`, `web application with React and Redux`, `React library`, `command line tool`.",
  "scripts": {
    "test": "NODE_ENV=test nyc ava",
    "test:watch": "NODE_ENV=test ava -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postcover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "bin",
    "npm",
    "template"
  ],
  "keywords": [
    "template",
    "react",
    "redux",
    "saga",
    "cli",
    "generate",
    "scaffolding",
    "babel",
    "webpack",
    "webpack-dashboard",
    "webpack-dev-server",
    "eslint",
    "ava",
    "enzyme",
    "css-modules",
    "flow"
  ],
  "bin": {
    "dish": "./bin/dish.js"
  },
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "url": "git+https://github.com/abouthiroppy/dish.git",
    "type": "git"
  },
  "author": "abouthiroppy <git@about-hiroppy.com> (http://about-hiroppy.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abouthiroppy/dish/issues"
  },
  "homepage": "https://github.com/abouthiroppy/dish#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "cross-spawn": "^5.0.0",
    "fs-extra": "^1.0.0",
    "inquirer": "^2.0.0",
    "path-exists": "^3.0.0",
    "rc": "^1.1.6"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel-eslint": "^7.0.0",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^3.10.0",
    "eslint-config-sky": "^1.4.0",
    "nyc": "^10.0.0",
    "pify": "^2.3.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true
  }
}
