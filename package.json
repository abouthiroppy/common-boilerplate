{
  "name": "my-dish",
  "version": "1.6.0",
  "description": "Generates various templates. React, React-Redux, CLI, React-library, etc...",
  "scripts": {
    "test": "NODE_ENV=test nyc ava",
    "test:watch": "NODE_ENV=test ava -w",
    "test:appveyor": "ava",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postcover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "bin",
    "npm",
    "templates"
  ],
  "keywords": [
    "template",
    "react",
    "redux",
    "saga",
    "cli",
    "generate",
    "scaffolding",
    "babel",
    "webpack",
    "webpack-dashboard",
    "webpack-dev-server",
    "eslint",
    "ava",
    "enzyme",
    "css-modules",
    "flow"
  ],
  "bin": {
    "dish": "./bin/dish.js"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "url": "git+https://github.com/abouthiroppy/dish.git",
    "type": "git"
  },
  "author": "abouthiroppy <git@about-hiroppy.com> (http://about-hiroppy.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abouthiroppy/dish/issues"
  },
  "homepage": "https://github.com/abouthiroppy/dish#readme",
  "dependencies": {
    "@my-dish/packer": "^0.2.4",
    "chalk": "^1.1.3",
    "command-exists": "^1.2.2",
    "fs-extra": "^1.0.0",
    "inquirer": "^2.0.0",
    "ora": "^0.4.1",
    "path-exists": "^3.0.0"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "babel-eslint": "^7.2.1",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^3.13.0",
    "eslint-config-sky": "^1.4.3",
    "nyc": "^10.2.0",
    "pify": "^2.3.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true
  }
}
